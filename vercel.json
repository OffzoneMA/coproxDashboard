{
    "version": 2,
    "builds": [
        {
            "src": "server/index.js",
            "use": "@vercel/node",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "functions": {
        "server/index.js": {
            "maxDuration": 30
        }
    },
    "installCommand": "cd server && npm install --production=false"
}